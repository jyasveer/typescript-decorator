{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "declaration": true,
      "stripInternal": true,
      "skipLibCheck": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "allowSyntheticDefaultImports": true,
      "sourceMap": true,
      "inlineSources": true,
      "noEmitHelpers": false,
      "strictNullChecks": false,
      "noImplicitAny": true,
      "baseUrl": ".",
      "rootDir": ".",
      "outDir": "dist",
      "lib": [
        "es2015",
        "dom"
      ]
    },
    "awesomeTypescriptLoaderOptions": {
      "forkChecker": true,
      "useWebpackText": true
    },
    "angularCompilerOptions": {
      "strictMetadataEmit": true,
      "skipTemplateCodegen": true
    },
    "compileOnSave": false,
    "buildOnSave": false,
    "atom": { "rewriteTsconfig": false }
  }
  